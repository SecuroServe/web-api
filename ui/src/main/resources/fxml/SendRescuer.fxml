<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane stylesheets="@../css/flatterfx.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox prefHeight="555.0" prefWidth="1173.0">
         <children>
            <VBox prefHeight="535.0" prefWidth="350.0">
               <children>
                    <Label prefHeight="23.0" prefWidth="369.0" text="Select calimity" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                    <TableView fx:id="calamityTableView" prefHeight="450.0" prefWidth="329.0" />
                  <HBox prefWidth="200.0">
                     <children>
                          <Button fx:id="backButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="123.0" text="&lt; Back" />
                          <Button fx:id="selectCalamityButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="123.0" text="Select">
                           <HBox.margin>
                              <Insets left="110.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </HBox.margin>
            </VBox>
              <Separator orientation="VERTICAL" prefHeight="555.0" prefWidth="6.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Separator>
            <VBox prefHeight="200.0" prefWidth="350.0">
               <children>
                    <Label fx:id="selectedCalamityLabel" prefHeight="23.0" prefWidth="365.0" text="Selected Calamity">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                    <ListView fx:id="selectedRescuerListView" prefHeight="450.0" prefWidth="296.0" />
                  <HBox prefWidth="200.0">
                     <children>
                          <Button fx:id="removeRescuerButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="113.0" text="Remove">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                          <Button fx:id="doneButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="234.0" text="Inform selected" />
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </HBox.margin>
            </VBox>
              <Separator orientation="VERTICAL" prefHeight="555.0" prefWidth="6.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Separator>
            <VBox prefHeight="200.0" prefWidth="350.0">
               <children>
                    <Label prefHeight="23.0" prefWidth="396.0" text="Select Rescuer" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                    <TableView fx:id="rescuerTableView" prefHeight="434.0" prefWidth="329.0" />
                    <Button fx:id="addRescuerButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="93.0" text="Add">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
    </children>
</Pane>
